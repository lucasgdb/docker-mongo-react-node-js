{
	"name": "template",
	"private": true,
	"version": "1.0.0",
	"author": "Lucas Bittencourt <lucasgdbittencourt@gmail.com>",
	"repository": "git@github.com:lucasgdb/docker-mongo-react-node-js.git",
	"license": "MIT",
	"scripts": {
		"api:start": "yarn workspace @template/api start",
		"web:start": "yarn workspace @template/web start",
		"start": "concurrently --kill-others \"yarn api:start\" \"yarn web:start\"",
		"api:production": "yarn workspace @template/api run start:production",
		"web:production": "yarn workspace @template/web run start:production",
		"api:build": "yarn workspace @template/api run build",
		"web:build": "yarn workspace @template/web run build",
		"build": "yarn workspaces run build"
	},
	"devDependencies": {
		"concurrently": "^5.1.0"
	},
	"workspaces": [
		"./packages/*"
	]
}
